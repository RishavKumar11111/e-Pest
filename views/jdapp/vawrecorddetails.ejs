<div class="right_col" role="main" ng-controller="myJDAPPVAWRecordDCtrl" style="min-height: 100vh">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <h3>VAW Details</h3>
            </div>
            <div class="title_right">
                <!-- <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Search for...">
                        <span class="input-group-btn">
                            <button class="btn btn-default" type="button">Go!</button>
                        </span>
                    </div>
                </div> -->
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>VAW Details <small>(Enter VAW Code)</small></h2>
                        <ul class="nav navbar-right panel_toolbox">
                            <!-- <li>
                                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                            </li> -->
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <br />
                        <div class="col-md-12 col-sm-12 col-xs-12" style="height: 44px">
                            <div class="col-md-3 col-sm-3 col-xs-12"></div>
                            <div class="col-md-6 col-sm-6 col-xs-12">
                                <div class="form-group">
                                    <label style="text-align: left; white-space: nowrap; padding-top: 8px" class="control-label col-md-4 col-sm-4 col-xs-12">Enter VAW Code : <span style="color: red">*</span></label>
                                    <div class="col-md-8 col-sm-8 col-xs-12">
                                        <input type="text" class="form-control" ng-model="vawCode" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3 col-sm-3 col-xs-12"></div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="ln_solid"></div>
                        <div class="col-md-12 col-sm-12 col-xs-12 text-center">
                            <input type="button" class="btn btn-success" value="Get VAW Details" ng-disabled="vawCode == null || vawCode == undefiend " ng-click="getVAWRecordDetails(vawCode)" />
                        </div>
                        <div class="clearfix"></div>
                        <hr />
                        <div class="col-md-12 col-sm-12 col-xs-12" ng-show="vawDetails.length > 0">
                            <div class="form-group" style="text-align: center">
                                <div class="table table-responsive">
                                    <table class="table table-hover table-striped jambo_table bulk_action">
                                        <thead>
                                            <tr class="headings">
                                                <td style="vertical-align: middle"><b>Serial No.</b></td>
                                                <td style="vertical-align: middle"><b>District</b></td>
                                                <td style="vertical-align: middle"><b>Block Details</b></td>
                                            </tr>
                                        </thead>
                                        <tr ng-repeat="(key, value) in vawDetails | orderBy: 'DistrictName' | groupBy: 'DistrictName'">
                                            <td style="vertical-align: middle">{{$index + 1}}</td>
                                            <td style="vertical-align: middle"><b>{{key | capitalize}}</b></td>
                                            <td>
                                                <table class="table table-hover table-striped jambo_table bulk_action">
                                                    <thead>
                                                        <tr class="headings">
                                                            <td><b>Block Name</b></td>
                                                            <td><b>VAW Details</b></td>
                                                        </tr>
                                                    </thead>
                                                    <tr ng-repeat="(i, j) in value | orderBy: 'BlockName'| groupBy: 'BlockName'">
                                                        <td style="vertical-align: middle">{{i | capitalize}}</td>
                                                        <td>
                                                            <table class="table table-hover table-striped jambo_table bulk_action">
                                                                <thead>
                                                                    <tr class="headings">
                                                                        <td><b>VAW Name</b></td>
                                                                        <td><b>VAW MobileNo</b></td>
                                                                        <td><b>GP Details</b></td>
                                                                    </tr>
                                                                </thead>
                                                                <tr ng-repeat="(k, l) in j | orderBy: 'VAWName'| groupBy: 'VAWName'">
                                                                    <td style="vertical-align: middle">{{k}}</td>
                                                                    <td style="vertical-align: middle">{{l[0].VAWMobileNo}}</td>
                                                                    <td>
                                                                        <table class="table table-hover table-striped jambo_table bulk_action">
                                                                            <thead>
                                                                                <tr class="headings">
                                                                                    <td><b>Serial No.</b></td>
                                                                                    <td><b>GP Name</b></td>
                                                                                </tr>
                                                                            </thead>
                                                                            <tr ng-repeat="(m, n) in l | orderBy: 'GPName'| groupBy: 'GPName'">
                                                                                <td>{{$index + 1}}</td>
                                                                                <td>{{m | capitalize}}</td>
                                                                            </tr>
                                                                        </table>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>