                                    <table class="table table-hover table-striped jambo_table bulk_action">
                                        <thead>
                                            <tr class="headings">
                                                <td style="vertical-align: middle"><b>Serial No.</b></td>
                                                <td style="vertical-align: middle"><b>VAW Code</b></td>
                                                <td style="vertical-align: middle"><b>GP Details</b></td>
                                            </tr>
                                        </thead>
                                        <tr ng-repeat="(key, value) in pestDetails | orderBy: 'VAWCode' | groupBy: 'VAWCode'">
                                            <td><b>{{$index + 1}}</b></td>
                                            <td><b>{{key}}</b></td>
                                            <td>
                                                <table class="table table-hover table-striped jambo_table bulk_action">
                                                    <thead>
                                                        <tr class="headings">
                                                            <td><b>Serial No.</b></td>
                                                            <td><b>GP</b></td>
                                                            <td><b>ReferenceNo Details</b></td>
                                                        </tr>
                                                    </thead>
                                                    <tr ng-repeat="(i, j) in value | orderBy: 'GPName' | groupBy: 'GPName'">
                                                        <td><b>{{$index + 1}}</b></td>
                                                        <td><b>{{i | capitalize}}</b></td>
                                                        <td>
                                                            <table class="table table-hover table-striped jambo_table bulk_action">
                                                                <thead>
                                                                    <tr class="headings">
