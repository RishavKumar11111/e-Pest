        <div class="modal fade" id="CDModal" role="dialog">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title text-center">Crop Details Section</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row" ng-show="cd != null">
                            <div class="col-md-1 col-sm-1 col-xs-12"></div>
                            <div class="form-horizontal col-md-10 col-sm-10 col-xs-12">
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Reference No. :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.ReferenceNo}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">District :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.DistrictName | capitalize}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Block :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.BlockName | capitalize}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">GP :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.GPName | capitalize}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Village :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.VillageName | capitalize}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left; padding-top: 8px" class="control-label col-md-6 col-sm-6 col-xs-12">Mobile No. :</label>
                                    <label style="text-align: left; padding-top: 8px" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.MobileNo}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Season :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.Season}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Crop Category :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.CategoryName}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Crop :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.CropName}}</label>
                                </div>
                                <div class="form-group">
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">Crop Stage :</label>
                                    <label style="text-align: left" class="control-label col-md-6 col-sm-6 col-xs-12">{{cd.CropStageName}}</label>
                                </div>
                            </div>
                            <div class="col-md-1 col-sm-1 col-xs-12"></div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" id="PLDModal" role="dialog">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title text-center">Photo & Location Details Section</h4>
                    </div>
                    <div class="modal-body">
                        <div class="row" ng-show="pld != null">
                            <div class="form-horizontal">
                                <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                    <label style="text-align: left" class="control-label col-md-4 col-sm-4 col-xs-12">Reference No. :</label>
                                    <label style="text-align: left" class="control-label col-md-4 col-sm-4 col-xs-12">{{pld.ReferenceNo}}</label>
                                    <label class="control-label col-md-4 col-sm-4 col-xs-12"></label>
                                </div>
                                <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                    <label style="text-align: left" class="control-label col-md-4 col-sm-4 col-xs-12">Fixed Land Photo :</label>
                                    <img id="flp" class="col-md-5 col-sm-5 col-xs-12" />
                                    <div class="col-md-3 col-sm-3 col-xs-12">
                                        <label class="control-label">Latitude: {{pld.FixedLandLatitude}}</label>
                                        <label class="control-label">Longitude: {{pld.FixedLandLongitude}}</label>
                                    </div>
                                </div>
                                <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                    <label style="text-align: left" class="control-label col-md-4 col-sm-4 col-xs-12">1st Random Land Photo :</label>
                                    <div ng-show="pld.RLP1 != null">
                                        <img id="rlp1" class="col-md-5 col-sm-5 col-xs-12" />
                                    </div>
                                    <div ng-show="pld.RLP1 == null" style="padding-top: 8px" class="col-md-5 col-sm-5 col-xs-12"><b>NA</b></div>
                                    <div class="col-md-3 col-sm-3 col-xs-12">
                                        <label class="control-label">Latitude: {{pld.RandomLandLatitude1 == null ? 'NA' : pld.RandomLandLatitude1}}</label>
                                        <label class="control-label">Longitude: {{pld.RandomLandLongitude1 == null ? 'NA' : pld.RandomLandLongitude1}}</label>
                                    </div>
                                </div>
                                <div class="form-group col-md-12 col-sm-12 col-xs-12">
                                    <label style="text-align: left" class="control-label col-md-4 col-sm-4 col-xs-12">2nd Random Land Photo :</label>
                                    <div ng-show="pld.RLP2 != null">
                                        <img id="rlp2" class="col-md-5 col-sm-5 col-xs-12" />
                                    </div>
                                    <div ng-show="pld.RLP2 == null" style="padding-top: 8px" class="col-md-5 col-sm-5 col-xs-12"><b>NA</b></div>
                                    <div class="col-md-3 col-sm-3 col-xs-12">
                                        <label class="control-label">Latitude: {{pld.RandomLandLatitude2 == null ? 'NA' : pld.RandomLandLatitude2}}</label>
                                        <label class="control-label">Longitude: {{pld.RandomLandLongitude2 == null ? 'NA' : pld.RandomLandLongitude2}}</label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
