    <script>
        var app = angular.module('myADOApp', ['ngRoute', 'angular.filter']);
        app.controller('myADOCtrl', function($scope, $http) {

        });
        app.filter('capitalize', function () {
            return function (input) {
                if (input != null) {
                    input = input.toLowerCase().split(' ');
                    for (var i = 0; i < input.length; i++) {
                        input[i] = input[i].charAt(0).toUpperCase() + input[i].substring(1);
                    }
                    return input.join(' ');
                }
                else {
                    return false;
                }
            }
            // return function (input) {
            //     return (!!input) ? input.charAt(0).toUpperCase() + input.substr(1).toLowerCase() : '';
            // }
        });
        app.config(['$routeProvider', function($routeProvider) {
            $routeProvider
            .when("/", {
               templateUrl: "ado/dashboard",
               controller: 'myADODashboardCtrl'
            }).when("/home", {
                templateUrl: "ado/home",
                controller: 'myADOHomeCtrl'
            }).when("/emergencyCase", {
                templateUrl: "ado/emergencycase",
                controller: 'myADOEMRCtrl'
            }).when("/messageForJDAPP", {
                templateUrl: "ado/messageforjdapp",
                controller: 'myADOMFJCtrl'
            }).when("/changePassword", {
                templateUrl: "ado/changePassword",
                controller: 'myADOCPCtrl'
            }).when("/aaoDetails", {
               templateUrl: "ado/aaoDetails",
               controller: 'myADOAAODetailsCtrl'
            }).when("/vawDetails", {
               templateUrl: "ado/vawDetails",
               controller: 'myADOVAWDetailsCtrl'
            }).when("/vawGPTargetDetails", {
               templateUrl: "ado/vawGPTargetDetails",
               controller: 'myADOVAWGTDetailsCtrl'
            }).when("/viewPestDetails", {
                templateUrl: "ado/viewPestDetails",
                controller: 'myADOVPDCtrl'
            }).when("/lighttTrapcatchReport", {
               templateUrl: "ado/lighttTrapcatchReport",
               controller: 'myADOLTCRCtrl'
            }).when("/vawInspectionReport", {
               templateUrl: "ado/vawInspectionReport",
               controller: 'myADOVAWIRCtrl'
            }).when("/emergencyCaseReport", {
               templateUrl: "ado/emergencyCaseReport",
               controller: 'myADOEMRCRCtrl'
            }).when("/complianceReport", {
                templateUrl: "ado/complianceReport",
                controller: 'myADOComplianceCtrl'
            }).otherwise({
                  redirectTo: "/"
            });
        }]);
        app.directive('loading', ['$http', function ($http) {
            return {
                restrict: 'A',
                template: '<div class="lds-eclipse" style="background: white; z-index: 99999; position: fixed"><div style="margin: 15% 43%"></div></div>',
                // Loading&#8230;
                link: function (scope, elm, attrs) {
                    scope.isLoading = function () {
                        return $http.pendingRequests.length > 0;
                    };
                    scope.$watch(scope.isLoading, function (v) {
                        if (v) {
                            elm.css('display', 'block');
                        } else {
                            elm.css('display', 'none');
                        }
                    });
                }
            };
        }]);
    </script>
