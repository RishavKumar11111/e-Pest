    <script>
        var app = angular.module('myCDAOApp', ['ngRoute', 'angular.filter']);
        app.controller('myCDAOCtrl', function($scope, $http) {
            
            $scope.getDistrict = function() {
                $http.get('http://localhost:3000/cdao/getDistrict').then(function success(response) {
                    $scope.district = response.data[0].DistrictName;
                }, function error(response) {
                    console.log(response.status);
                }).catch(function err(error) {
                    console.log('An error occurred...', error);
                });
            };

        });
        app.filter('capitalize', function () {
            return function (input) {
                if (input != null) {
                    input = input.toLowerCase().split(' ');
                    for (var i = 0; i < input.length; i++) {
                        input[i] = input[i].charAt(0).toUpperCase() + input[i].substring(1);
                    }
                    return input.join(' ');
                }
                else {
                    return false;
                }
            }
            // return function (input) {
            //     return (!!input) ? input.charAt(0).toUpperCase() + input.substr(1).toLowerCase() : '';
            // }
        });
        app.config(['$routeProvider', function($routeProvider) {
            $routeProvider
            .when("/", {
               templateUrl: "cdao/dashboard",
               controller: 'myCDAODashboardCtrl'
            }).when("/home", {
                templateUrl: "cdao/home",
                controller: 'myCDAOHomeCtrl'
            }).when("/aaoDetailEntry", {
                templateUrl: "cdao/aaoDetailEntry",
                controller: 'myAAODECtrl'
            }).when("/changePassword", {
                templateUrl: "cdao/changePassword",
                controller: 'myCDAOCPCtrl'
            }).when("/aaoDetails", {
                templateUrl: "cdao/aaoDetails",
                controller: 'myCDAOADReportCtrl'
            }).when("/vawDetails", {
                templateUrl: "cdao/vawDetails",
                controller: 'myCDAOVDReportCtrl'
            }).when("/vawGPTargetDetails", {
                templateUrl: "cdao/vawGPTargetDetails",
                controller: 'myCDAOVDReportCtrl'
            }).when("/viewModifyPestDetails", {
                templateUrl: "cdao/viewModifyPestDetails",
                controller: 'myCDAOVMPDCtrl'
            }).when("/viewPestDetails", {
                templateUrl: "cdao/viewPestDetails",
                controller: 'myCDAOVPDCtrl'
            }).when("/lighTtrapCatchReport", {
                templateUrl: "cdao/lighTtrapCatchReport",
                controller: 'myCDAOLTCRCtrl'
            }).when("/vawInspectionReport", {
                templateUrl: "cdao/vawInspectionReport",
                controller: 'myCDAOVAWIRCtrl'
            }).when("/emergencyCaseReport", {
                templateUrl: "cdao/emergencyCaseReport",
                controller: 'myCDAOEMRCRCtrl'
            }).otherwise({
                  redirectTo: "/"
            });
        }]);
        app.directive('loading', ['$http', function ($http) {
            return {
                restrict: 'A',
                template: '<div class="lds-eclipse" style="background: white; z-index: 99999; position: fixed"><div style="margin: 15% 43%"></div></div>',
                // Loading&#8230;
                link: function (scope, elm, attrs) {
                    scope.isLoading = function () {
                        return $http.pendingRequests.length > 0;
                    };
                    scope.$watch(scope.isLoading, function (v) {
                        if (v) {
                            elm.css('display', 'block');
                        } else {
                            elm.css('display', 'none');
                        }
                    });
                }
            };
        }]);
    </script>
