    <script>
        var app = angular.module('myVAWApp', ['angular.filter', 'ngAnimate', 'ui.router']);
        app.service('vawService', function() {
            var referenceNo;
            var cropCode;
            return {
                getReferenceNo: function() {
                    return referenceNo;
                },
                setReferenceNo: function(refNo) {
                    referenceNo = refNo;
                },
                getCropCode: function() {
                    return cropCode;
                },
                setCropCode: function(cc) {
                    cropCode = cc;
                }
            };
        });
        app.controller('myVAWCtrl', function($scope, $http, vawService) {
            
        });
        app.filter('capitalize', function () {
            return function (input) {
                if (input != null) {
                    input = input.toLowerCase().split(' ');
                    for (var i = 0; i < input.length; i++) {
                        input[i] = input[i].charAt(0).toUpperCase() + input[i].substring(1);
                    }
                    return input.join(' ');
                }
                else {
                    return false;
                }
            }
            // return function (input) {
            //     return (!!input) ? input.charAt(0).toUpperCase() + input.substr(1).toLowerCase() : '';
            // }
        });
        app.config(function($stateProvider, $urlRouterProvider) {
            $stateProvider
            .state('home', {
                url: '/home',
                templateUrl: 'vawApp/home',
                controller: 'myVAWAppHomeCtrl'
            }).state('dashboard', {
                url: '/dashboard',
                templateUrl: 'vawApp/dashboard',
                controller: 'myVAWAppDashboardCtrl'
            }).state('synchronize', {
                url: '/synchronize',
                templateUrl: 'vawApp/synchronize',
                controller: 'myVAWAppSyncCtrl'
            }).state('changePassword', {
                url: '/changePassword',
                templateUrl: 'vawApp/changepassword',
                controller: 'myVAWAppCPCtrl'
            }).state('dashboard.cropDetails', {
                url: '/cropDetails',
                templateUrl: 'vawApp/cropdetails'
            }).state('dashboard.pestDetails', {
                url: '/pestDetails',
                templateUrl: 'vawApp/pestdetails'
            }).state('dashboard.capturePhoto', {
                url: '/capturePhoto',
                templateUrl: 'vawApp/capturephoto'
            }).state('offline', {
                url: '/offline',
                templateUrl: 'vawApp/offline'
            });
            $urlRouterProvider.otherwise('/home');
        });
        app.directive('loading', ['$http', function ($http) {
            return {
                restrict: 'A',
                template: '<div class="lds-eclipse" style="background: white; z-index: 99999; position: fixed"><div style="margin: 50% 22%"></div></div>',
                // Loading&#8230;
                link: function (scope, elm, attrs) {
                    scope.isLoading = function () {
                        return $http.pendingRequests.length > 0;
                    };
                    scope.$watch(scope.isLoading, function (v) {
                        if (v) {
                            elm.css('display', 'block');
                        } else {
                            elm.css('display', 'none');
                        }
                    });
                }
            };
        }]);
    </script>
