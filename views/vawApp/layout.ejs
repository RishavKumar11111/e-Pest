            //     });
            // }
        };
        function setLocationHrefSync() {
            if (navigator.onLine) {
                location.href = 'http://localhost:3000/vawApp#!/synchronize';
            }
            else {
                location.href = 'http://localhost:3000/vawApp#!/offline';
            }                
        };
        function setLocationHrefCP() {
            if (navigator.onLine) {
                location.href = 'http://localhost:3000/vawApp#!/changePassword';
            }
            else {
                location.href = 'http://localhost:3000/vawApp#!/offline';
            }                
        };
